{
  "name": "@caliopen/frontend",
  "version": "0.5.4",
  "description": "CaliOpen UI is the user interface for CaliOpen.",
  "homepage": "https://caliopen.org/",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all build:clean --parallel --race -l start:dev:server start:dev:assets",
    "start:dev": "HAS_SSR=false npm-run-all -l build:clean --parallel --race start:dev:server start:dev:assets",
    "start:dev:server": "npm-run-all --parallel --race build:server:watch start:dev:server:watch",
    "start:dev:server:watch": "CALIOPEN_API_HOSTNAME=localhost CALIOPEN_WEB_SERVER_PORT=4001 CALIOPEN_WEB_SERVER_HOSTNAME=127.0.0.1 nodemon dist/server/index.js",
    "start:dev:assets": "NODE_ENV=development CALIOPEN_MOTD='credentials: dev / 123456' webpack-dev-server --public --progress --config webpack/webpack.config.browser.js --port 4000",
    "start:prod": "NODE_ENV=production ./bin/server",
    "start:desktop": "npm-run-all --parallel --race build:desktop:dev start:desktop:electron",
    "start:desktop:electron": "NODE_ENV=development electron ./electron/",
    "start:smartphone": "npm-run-all --parallel --race build:smartphone:dev start:smartphone:android",
    "start:smartphone:android": "cd cordova && nodemon --watch www --exec 'cordova emulate android --debug'",
    "start:mock-api": "../../../devtools/api-mock/bin/start",
    "build:clean": "if [ -d dist ];then rm -r dist;fi",
    "build:browser:test": "NODE_ENV=test CALIOPEN_PIWIK_SITE_ID=false webpack --config webpack/webpack.config.browser.js",
    "build:server:test": "NODE_ENV=test HAS_SSR=true webpack --config webpack/webpack.config.server.js",
    "build:server:watch": "NODE_ENV=development HAS_SSR=true webpack --watch --profile --progress --config webpack/webpack.config.server.js",
    "build:desktop:dev": "NODE_ENV=development webpack --watch --config webpack/webpack.config.electron.js",
    "build:smartphone:dev": "NODE_ENV=development webpack --watch --config webpack/webpack.config.cordova.js",
    "release": "npm-run-all release:web:browser release:web:server",
    "release:web:server": "NODE_ENV=production webpack --bail --progress --config webpack/webpack.config.server.js",
    "release:web:browser": "NODE_ENV=production webpack --bail -p --progress --config webpack/webpack.config.browser.js",
    "cordova": "cd cordova && cordova",
    "test": "npm-run-all lint test:unit test:functional",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:functional": "npm-run-all --sequential -n build:browser:test build:server:test test:functional:start",
    "test:functional:start": "npm-run-all -n --parallel --race start:mock-api test:functional:serve test:e2e:delay",
    "test:functional:serve": "NODE_ENV=test CALIOPEN_API_HOSTNAME=localhost ./bin/server",
    "test:e2e": "protractor ./test/functional/protractor.conf.js",
    "test:e2e:delay": "sleep 3 && protractor ./test/functional/protractor.conf.js",
    "test:e2e:init": "webdriver-manager update",
    "lint": "npm-run-all lint:js lint:sass",
    "lint:js": "eslint --ext .js --ext .jsx --ignore-pattern *.spec.js --ignore-pattern *.spec.jsx src/",
    "lint:sass": "sass-lint 'src/**/*.scss'",
    "i18n": "react-translate-scripts"
  },
  "author": "CaliOpen <contact@caliopen.org> (https://caliopen.org/)",
  "license": "MIT",
  "resolutions": {
    "react": "16.0.0",
    "react-dom": "16.0.0"
  },
  "dependencies": {
    "@gandi/react-translate": "^1.0.3",
    "argv": "^0.0.2",
    "async-validator": "^1.8.1",
    "axios": "^0.16.2",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "express-http-proxy": "^1.0.7",
    "font-awesome": "^4.6.3",
    "foundation-sites": "~6.3.0",
    "history": "^4.7.2",
    "iron": "^4.0.5",
    "jquery": "^3.2.1",
    "locale": "^0.1.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.0",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.19.1",
    "moment-timezone": "^0.5.13",
    "openpgp": "^2.5.12",
    "piwik-react-router": "^0.10.0",
    "prop-types": "^15.6.0",
    "rc-slider": "^8.3.2",
    "react": "^16.0.0",
    "react-country-region-selector": "^1.2.2",
    "react-datepicker": "^0.56.0",
    "react-dom": "^16.0.0",
    "react-loadable": "^5.2.4",
    "react-modal": "^3.0.3",
    "react-moment": "^0.6.5",
    "react-redux": "^5.0.6",
    "react-redux-notify": "^2.0.0",
    "react-redux-scroll": "^0.0.14",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-swipeable": "^4.1.0",
    "react-tappable": "^1.0.2",
    "react-visibility-sensor": "^3.11.0",
    "redux": "^3.7.2",
    "redux-axios-middleware": "^4.0.0",
    "redux-form": "^7.1.1",
    "reselect": "^3.0.1",
    "serve-favicon": "^2.4.5",
    "universal-url": "^1.0.0",
    "uuid": "^3.1.0",
    "xregexp": "^3.2.0",
    "zxcvbn": "^4.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "axios-mock-adapter": "^1.9.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-gettext-plugin": "^2.2.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cordova": "^7.1.0",
    "css-loader": "^0.28.7",
    "deep-freeze-strict": "^1.1.1",
    "electron": "^1.7.9",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.1.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.29.0",
    "http-server": "^0.10.0",
    "image-webpack-loader": "^3.4.2",
    "jasmine-spec-reporter": "^4.2.1",
    "jest-cli": "^21.2.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "null-loader": "^0.1.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "po2json": "^0.4.5",
    "postcss-loader": "^2.0.8",
    "protractor": "^5.2.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dev-utils": "^4.1.0",
    "react-markdown": "^2.5.0",
    "react-test-renderer": "^16.0.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.2",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.2",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/unit/setup.js",
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "globals": {
      "CALIOPEN_ENV": "test",
      "BUILD_TARGET": "none",
      "CALIOPEN_OPTIONS": {}
    }
  }
}
